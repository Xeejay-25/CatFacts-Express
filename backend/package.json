{
    "name": "catfacts-express-backend",
    "version": "1.0.0",
    "description": "Express.js backend for CatFacts application",
    "main": "dist/server.js",
    "scripts": {
        "build": "tsc",
        "start": "node dist/server.js",
        "dev": "concurrently \"tsc -w\" \"nodemon dist/server.js\"",
        "dev:ts": "npx ts-node-dev --respawn --transpile-only src/server.ts",
        "lint": "eslint . --ext .ts",
        "test": "jest"
    },
    "dependencies": {
        "express": "^4.21.0",
        "cors": "^2.8.5",
        "helmet": "^7.1.0",
        "morgan": "^1.10.0",
        "dotenv": "^16.4.5",
        "bcryptjs": "^2.4.3",
        "jsonwebtoken": "^9.0.2",
        "express-rate-limit": "^7.2.0",
        "express-validator": "^7.0.1",
        "sqlite3": "^5.1.6",
        "node-cache": "^5.1.2",
        "axios": "^1.6.8",
        "uuid": "^9.0.1",
        "compression": "^1.7.4"
    },
    "devDependencies": {
        "@types/node": "^20.11.30",
        "@types/express": "^4.17.21",
        "@types/cors": "^2.8.17",
        "@types/morgan": "^1.9.9",
        "@types/bcryptjs": "^2.4.6",
        "@types/jsonwebtoken": "^9.0.6",
        "@types/uuid": "^9.0.8",
        "@types/compression": "^1.7.5",
        "@typescript-eslint/eslint-plugin": "^7.3.1",
        "@typescript-eslint/parser": "^7.3.1",
        "eslint": "^8.57.0",
        "typescript": "^5.4.3",
        "ts-node-dev": "^2.0.0",
        "nodemon": "^3.1.0",
        "concurrently": "^8.2.2",
        "jest": "^29.7.0",
        "@types/jest": "^29.5.12"
    },
    "keywords": [
        "express",
        "typescript",
        "catfacts",
        "api",
        "backend"
    ],
    "author": "Student",
    "license": "MIT"
}